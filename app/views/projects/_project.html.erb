<div class= "project">
  <div class= "project_left_box">
    <div class= "project_title">
      <%= project.title %>
    </div>
    <div class= "project_reaction_box">
      <div class= "project_like">
        ☆：０
      </div>
      <div class= "project_read">
        👁：０
      </div>
    </div>
  </div>
  <div class= "project_right_box">
    <div class= "project_created_at">
      作成日：<%= l project.created_at %>
    </div>
    <div class= "project_owner">
      作成者：<%= project.owner.nickname %>
    </div>
  </div>
</div>